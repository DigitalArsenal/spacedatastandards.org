// https://www.celestrak.com/satcat/satcat-format.php

enum objectType: byte {
  /// 0
  PAYLOAD,
  /// 1
  ROCKET_BODY,
  /// 2
  DEBRIS,
  /// 3
  UNKNOWN
}

enum opsStatusCode: byte {
  /// +
  OPERATIONAL,
  /// -
  NONOPERATIONAL,
  /// P
  PARTIALLY_OPERATIONAL,
  /// B
  BACKUP_STANDBY,
  /// S
  SPARE,
  /// X
  EXTENDED_MISSION,
  /// D
  DECAYED,
  /// ?
  UNKNOWN
}

enum dataStatusCode: byte {
  /// NCE
  NO_CURRENT_ELEMENTS,
  /// NIE
  NO_INITIAL_ELEMENTS,
  /// NEA
  NO_ELEMENTS_AVAILABLE,
  /// OK
  OK
}

enum orbitType: byte {
  /// 0
  ORBIT,
  /// 1
  LANDING,
  /// 2
  IMPACT,
  /// 3
  DOCKED,
  /// 4
  ROUNDTRIP
}

enum massType: byte {
  DRY,
  WET
}

