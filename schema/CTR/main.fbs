/// Country Identity Message
table CTR {
  /// ISO 3166 Numeric code
  ID: string;
  
  /// Country name
  NAME: string;

  /// GENC code
  GENC_CODE: string;

  /// ISO 3166 Alpha-2 code
  ALPHA_2_CODE: string;

  /// ISO 3166 Alpha-3 code
  ALPHA_3_CODE: string;

  /// Stanag code
  STANAG_CODE: string;

  /// Internet country code top-level domain (ccTLD)
  INTERNET_CCTLD: string;

  /// Additional comments
  COMMENT: string;
}

table CTRCOLLECTION {
  RECORDS: [CTR];
}

root_type CTRCOLLECTION;
file_identifier "$CTR";
