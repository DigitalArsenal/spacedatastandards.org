union RecordType {
  CRM, OMM, PRG, OSM,
  EPM, MPE, EME, OEM,
  VCM, CDM, IDM, SCM,
  PNM, CSM, HYP, LCC,
  ROC, EOP, CAT, OCM,
  CTR, TIM, MET, PLD,
  EOO, SIT, RFM, BOV,
  LDM, TDM, SPW
}  // Union of all record types

table Record {
  value: RecordType; 
}

/// Collection of Standard Records
table REC {
  version: string;
  standard: string;
  RECORDS: [Record]; 
}

table RECCOLLECTION {
  RECORDS: [REC];
}

root_type REC;
file_identifier "$REC";