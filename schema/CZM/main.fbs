  POSITIONS_CARTESIAN:[double];
  /// Minimum heights per position
  MINIMUM_HEIGHTS:[double];
  /// Maximum heights per position
  MAXIMUM_HEIGHTS:[double];
  /// Granularity in radians
  GRANULARITY:double;
  /// Fill flag
  FILL:bool;
  /// Surface material
  MATERIAL:CZMMaterial;
  /// Outline flag
  OUTLINE:bool;
  /// Outline color
  OUTLINE_COLOR:CZMColor;
  /// Outline width
  OUTLINE_WIDTH:double;
  /// Shadow mode
  SHADOWS:string;
}

/// A CZML Packet describing an entity and its properties
table CZMPacket {
  /// Unique identifier for this object
  ID:string;
  /// Human-readable name
  NAME:string;
  /// Parent packet ID
  PARENT:string;
  /// Description (HTML allowed)
  DESCRIPTION:string;
  /// Availability interval (ISO 8601 interval)
  AVAILABILITY:string;
  /// Position as cartographic degrees
  POSITION_CARTOGRAPHIC_DEGREES:CZMCartographicDegrees;
  /// Position as Cartesian
  POSITION_CARTESIAN:CZMCartesian;
  /// Time-tagged positions [time, lon, lat, height, time, lon, lat, height, ...]
  POSITION_EPOCH:string;
  /// Time-tagged cartographic degree values
  POSITION_CARTOGRAPHIC_DEGREES_ARRAY:[double];
  /// Time-tagged Cartesian values
  POSITION_CARTESIAN_ARRAY:[double];
  /// Billboard properties
  BILLBOARD:CZMBillboard;
  /// Label properties
  LABEL:CZMLabel;
  /// Point properties
  POINT:CZMPoint;
  /// Polyline properties
  POLYLINE:CZMPolyline;
  /// Polygon properties
  POLYGON:CZMPolygon;
  /// Model properties
  MODEL:CZMModel;
  /// Path properties
  PATH:CZMPath;
  /// Ellipse properties
  ELLIPSE:CZMEllipse;
  /// Orientation (quaternion)
  ORIENTATION:CZMOrientation;
  /// Suggested camera offset
  VIEW_FROM:CZMViewFrom;
  /// Whether to delete this object
  DELETE:bool;
  /// Box properties
  BOX:CZMBox;
  /// Corridor properties
  CORRIDOR:CZMCorridor;
  /// Cylinder properties
  CYLINDER:CZMCylinder;
  /// Ellipsoid properties
  ELLIPSOID:CZMEllipsoid;
  /// Polyline volume properties
  POLYLINE_VOLUME:CZMPolylineVolume;
  /// Rectangle properties
  RECTANGLE:CZMRectangle;
  /// 3D Tileset properties
  TILESET:CZMTileset;
  /// Wall properties
  WALL:CZMWall;
  /// Position interpolation settings
  POSITION_INTERPOLATION:CZMInterpolation;
  /// Position reference frame (FIXED or INERTIAL)
  POSITION_REFERENCE_FRAME:string;
  /// Position reference to another entity
  POSITION_REFERENCE:string;
  /// Orientation epoch (ISO 8601)
  ORIENTATION_EPOCH:string;
  /// Orientation sampled data [t, x, y, z, w, t, x, y, z, w, ...]
  ORIENTATION_ARRAY:[double];
  /// Orientation interpolation settings
  ORIENTATION_INTERPOLATION:CZMInterpolation;
  /// Orientation reference to another entity
  ORIENTATION_REFERENCE:string;
  /// Generic bag for all time-dynamic (non-static) properties
  DYNAMIC_PROPERTIES:[CZMDynamicProperty];
}

/// CZML Document
table CZM {
  /// Document-level name
  NAME:string;
  /// Document-level version
  VERSION:string;
  /// Clock settings - current time (ISO 8601)
  CLOCK_CURRENT_TIME:string;
  /// Clock settings - interval (ISO 8601 interval)
  CLOCK_INTERVAL:string;
  /// Clock settings - multiplier
  CLOCK_MULTIPLIER:double;
  /// Clock range
  CLOCK_RANGE:string;
  /// Clock step
  CLOCK_STEP:string;
  /// All packets in the document
  PACKETS:[CZMPacket];
}

root_type CZM;
file_identifier "$CZM";