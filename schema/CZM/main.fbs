// CZML (Cesium Language)
//
// CZML is a JSON-based format for describing time-dynamic 3D scenes,
// primarily for use with CesiumJS. A CZML document is an array of packets
// that describe objects and their properties over time.
// Reference: https://github.com/AnalyticalGraphicsInc/czml-writer/wiki/CZML-Guide

enum CZMHeightReference : byte {
  NONE,
  CLAMP_TO_GROUND,
  RELATIVE_TO_GROUND
}

enum CZMClassificationType : byte {
  TERRAIN,
  CESIUM_3D_TILE,
  BOTH
}

enum CZMLabelStyle : byte {
  FILL,
  OUTLINE,
  FILL_AND_OUTLINE
}

enum CZMHorizontalOrigin : byte {
  LEFT,
  CENTER,
  RIGHT
}

enum CZMVerticalOrigin : byte {
  BASELINE,
  BOTTOM,
  CENTER,
  TOP
}

enum CZMShadowMode : byte {
  DISABLED,
  ENABLED,
  CAST_ONLY,
  RECEIVE_ONLY
}

enum CZMColorBlendMode : byte {
  HIGHLIGHT,
  REPLACE,
  MIX
}

enum CZMCornerType : byte {
  ROUNDED,
  MITERED,
  BEVELED
}

enum CZMArcType : byte {
  NONE,
  GEODESIC,
  RHUMB
}

enum CZMStripeOrientation : byte {
  HORIZONTAL,
  VERTICAL
}

enum CZMClockRange : byte {
  UNBOUNDED,
  CLAMPED,
  LOOP_STOP
}

enum CZMClockStep : byte {
  TICK_DEPENDENT,
  SYSTEM_CLOCK_MULTIPLIER,
  SYSTEM_CLOCK
}

/// Time interval for time-dynamic properties
table CZMInterval {
  /// ISO 8601 interval string (e.g. "2012-03-15T10:00:00Z/2012-03-16T10:00:00Z")
  INTERVAL:string;
}

/// Interpolation settings for sampled data
table CZMInterpolation {
  /// Algorithm: LINEAR, LAGRANGE, HERMITE
  ALGORITHM:string;
  /// Polynomial degree (1=linear, 5=typical Lagrange)
  DEGREE:int;
}

/// Dynamic value types for time-varying properties
enum CZMDynValueType : byte {
  NUMBER,
  BOOLEAN,
  STRING,
  COLOR,
  CARTESIAN3,
  CARTESIAN2,
  NEAR_FAR_SCALAR,
  UNIT_QUATERNION
}

/// A single interval in a time-varying property
table CZMDynInterval {
  /// ISO 8601 interval string
  INTERVAL:string;
  /// Value type
  VALUE_TYPE:CZMDynValueType;
  /// Numeric value (when VALUE_TYPE is NUMBER)
  NUMBER_VALUE:double;
  /// Boolean value (when VALUE_TYPE is BOOLEAN)
  BOOLEAN_VALUE:bool;
  /// String value (when VALUE_TYPE is STRING)
  STRING_VALUE:string;
  /// Color value (when VALUE_TYPE is COLOR)
  COLOR_VALUE:CZMColor;
  /// Array value for cartesian/nearFarScalar types
  ARRAY_VALUE:[double];
}

/// Sampled time-varying data
table CZMDynSampled {
  /// Reference epoch (ISO 8601)
  EPOCH:string;
  /// Value type determines stride through DATA
  VALUE_TYPE:CZMDynValueType;
  /// Interleaved [time, value(s), ...] â€” stride depends on VALUE_TYPE
  DATA:[double];
  /// Interpolation settings
  INTERPOLATION:CZMInterpolation;
}

/// A single time-dynamic property (non-static value)
table CZMDynamicProperty {
  /// Dotted path name, e.g. "billboard.scale", "point.color"
  NAME:string;
  /// Sampled data (when property uses epoch + data array)
  SAMPLED:CZMDynSampled;
  /// Interval-based values
  INTERVALS:[CZMDynInterval];
  /// Reference to another entity's property
  REFERENCE:string;
}

/// Cartographic position (longitude, latitude, height in radians/meters)
table CZMCartographicDegrees {
  /// Longitude in degrees
  LONGITUDE:double;
  /// Latitude in degrees
  LATITUDE:double;
  /// Height in meters above WGS84 ellipsoid
  HEIGHT:double;
}

/// Cartesian position (X, Y, Z in meters)
table CZMCartesian {
  /// X component in meters (Earth-fixed)
  X:double;
  /// Y component in meters (Earth-fixed)
  Y:double;
  /// Z component in meters (Earth-fixed)
  Z:double;
}

/// RGBA color
table CZMColor {
  /// Red component (0-255)
  RED:ubyte;
  /// Green component (0-255)
  GREEN:ubyte;
  /// Blue component (0-255)
  BLUE:ubyte;
  /// Alpha component (0-255)
  ALPHA:ubyte;
}

/// Near/Far scalar for distance-based display
table CZMNearFarScalar {
  /// Near distance in meters
  NEAR_DISTANCE:double;
  /// Value at near distance
  NEAR_VALUE:double;
  /// Far distance in meters
  FAR_DISTANCE:double;
  /// Value at far distance
  FAR_VALUE:double;
}

/// Distance display condition
table CZMDistanceDisplayCondition {
  /// Near distance in meters
  NEAR_DISTANCE:double;
  /// Far distance in meters
  FAR_DISTANCE:double;
}

/// Orientation as unit quaternion
table CZMOrientation {
  /// X component
  UNIT_QUATERNION_X:double;
  /// Y component
  UNIT_QUATERNION_Y:double;
  /// Z component
  UNIT_QUATERNION_Z:double;
  /// W component
  UNIT_QUATERNION_W:double;
}

/// Suggested camera offset from entity
table CZMViewFrom {
  /// X offset in meters
  X:double;
  /// Y offset in meters
  Y:double;
  /// Z offset in meters
  Z:double;
}

// ---- Material System ----

/// Solid color material
table CZMSolidColorMaterial {
  /// Color
  COLOR:CZMColor;
}

/// Image material
table CZMImageMaterial {
  /// Image URI
  IMAGE:string;
  /// Repeat X
  REPEAT_X:double;
  /// Repeat Y
  REPEAT_Y:double;
  /// Color tint
  COLOR:CZMColor;
  /// Whether the image has transparency
  TRANSPARENT:bool;
}

/// Grid material
table CZMGridMaterial {
  /// Grid line color
  COLOR:CZMColor;
  /// Cell alpha
  CELL_ALPHA:double;
  /// Number of grid lines along X
  LINE_COUNT_X:double;
  /// Number of grid lines along Y
  LINE_COUNT_Y:double;
  /// Thickness of grid lines along X
  LINE_THICKNESS_X:double;
  /// Thickness of grid lines along Y
  LINE_THICKNESS_Y:double;
  /// Offset of grid lines along X
  LINE_OFFSET_X:double;
  /// Offset of grid lines along Y
  LINE_OFFSET_Y:double;
}

/// Stripe material
table CZMStripeMaterial {
  /// Stripe orientation
  ORIENTATION:string;
  /// Even color
  EVEN_COLOR:CZMColor;
  /// Odd color
  ODD_COLOR:CZMColor;
  /// Offset
  OFFSET:double;
  /// Number of times to repeat
  REPEAT_COUNT:double;
}

/// Checkerboard material
table CZMCheckerboardMaterial {
  /// Even color
  EVEN_COLOR:CZMColor;
  /// Odd color
  ODD_COLOR:CZMColor;
  /// Repeat X
  REPEAT_X:double;
  /// Repeat Y
  REPEAT_Y:double;
}

/// Surface material (used by polygon, ellipse, box, etc.)
table CZMMaterial {
  /// Solid color material
  SOLID_COLOR:CZMSolidColorMaterial;
  /// Image material
  IMAGE:CZMImageMaterial;
  /// Grid material
  GRID:CZMGridMaterial;
  /// Stripe material
  STRIPE:CZMStripeMaterial;
  /// Checkerboard material
  CHECKERBOARD:CZMCheckerboardMaterial;
}

/// Polyline outline material
table CZMPolylineOutlineMaterial {
  /// Line color
  COLOR:CZMColor;
  /// Outline color
  OUTLINE_COLOR:CZMColor;
  /// Outline width
  OUTLINE_WIDTH:double;
}

/// Polyline arrow material
table CZMPolylineArrowMaterial {
  /// Arrow color
  COLOR:CZMColor;
}

/// Polyline dash material
table CZMPolylineDashMaterial {
  /// Dash color
  COLOR:CZMColor;
  /// Gap color
  GAP_COLOR:CZMColor;
  /// Dash length in pixels
  DASH_LENGTH:double;
  /// Dash pattern (bitmask)
  DASH_PATTERN:int;
}

/// Polyline glow material
table CZMPolylineGlowMaterial {
  /// Glow color
  COLOR:CZMColor;
  /// Glow power
  GLOW_POWER:double;
  /// Taper power
  TAPER_POWER:double;
}

/// Polyline material (used by polyline, path)
table CZMPolylineMaterial {
  /// Solid color material
  SOLID_COLOR:CZMSolidColorMaterial;
  /// Polyline outline material
  POLYLINE_OUTLINE:CZMPolylineOutlineMaterial;
  /// Polyline arrow material
  POLYLINE_ARROW:CZMPolylineArrowMaterial;
  /// Polyline dash material
  POLYLINE_DASH:CZMPolylineDashMaterial;
  /// Polyline glow material
  POLYLINE_GLOW:CZMPolylineGlowMaterial;
}

