{
    "name": "spacedatastandards",
    "private": false,
    "version": "0.0.2+1679080426992",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "vite build && cp -R standards docs/standards && cp CNAME ./docs/CNAME && node ./scripts/convertMarkDown.mjs",
        "build:libraries": "rm code/*.zip && node ./scripts/buildSrcArchives.js",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "deploy": "npm run build && node ./scripts/updateVersion.mjs &&  npm run build:libraries && git add -A && git commit -m 'deploy' && git push"
    },
    "devDependencies": {
        "@octokit/types": "^6.34.0",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
        "@tsconfig/svelte": "^2.0.1",
        "autoprefixer": "^10.4.7",
        "globby": "^13.1.1",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.14",
        "rollup-plugin-commonjs": "^10.1.0",
        "showdown": "^2.1.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.7",
        "svelte-preprocess": "^4.9.8",
        "tailwindcss": "^3.0.24",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4",
        "vite": "^2.9.9",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-singlefile": "^0.9.0"
    },
    "dependencies": {
        "@octokit/rest": "^18.12.0",
        "@octokit/webhooks": "^9.24.0",
        "codemirror": "^5.65.5",
        "codemirror-grammar-mode": "^0.1.10",
        "file-saver": "^2.0.5",
        "isomorphic-fetch": "^3.0.0",
        "jszip": "^3.10.1",
        "localforage": "^1.10.0",
        "svelte-awesome": "^3.0.0",
        "svelte-spa-router": "^3.2.0"
    }
}