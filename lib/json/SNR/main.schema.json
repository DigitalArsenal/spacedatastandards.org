{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "definitions": {
    "SensorType" : {
      "type" : "string",
      "enum": ["RADAR_PULSE", "RADAR_DOPPLER", "RADAR_CW", "RADAR_AESA", "RADAR_PESA", "IR_SEARCH", "IR_IMAGING", "EO_TV", "EO_LLLTV", "LASER_DESIGNATOR", "LASER_RANGEFINDER", "RWR", "MAW", "SONAR_ACTIVE", "SONAR_PASSIVE", "ACOUSTIC"]
    },
    "SensorMode" : {
      "type" : "string",
      "enum": ["OFF", "STANDBY", "SEARCH", "ACQUISITION", "TRACK", "TRACK_WHILE_SCAN", "DESIGNATE", "JAMMED"]
