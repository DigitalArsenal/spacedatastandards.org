        "MAX_RANGE" : {
                "type" : "number"
              },
        "MIN_RANGE" : {
                "type" : "number"
              },
        "FOV_AZIMUTH" : {
                "type" : "number"
              },
        "FOV_ELEVATION" : {
                "type" : "number"
              },
        "ANGULAR_RESOLUTION" : {
                "type" : "number"
              },
        "RANGE_RESOLUTION" : {
                "type" : "number"
              },
        "UPDATE_RATE" : {
                "type" : "number"
              },
        "DETECTION_THRESHOLD" : {
                "type" : "number"
              },
        "AZIMUTH_SCAN_RATE" : {
                "type" : "number"
              },
        "ELEVATION_SCAN_RATE" : {
                "type" : "number"
              },
        "POWER" : {
                "type" : "number"
              },
        "FREQUENCY" : {
                "type" : "number"
              },
        "RESERVED" : {
                "type" : "array", "items" : {"type" : "integer", "minimum" : 0, "maximum" :255}
