{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "definitions": {
    "FluxQualifier" : {
      "type" : "string",
      "enum": ["OBSERVED", "BURST_ADJUSTED", "INTERPOLATED_EXTRAPOLATED", "NO_OBSERVATION", "CELESTRAK_INTERPOLATED"]
    },
    "F107DataType" : {
      "type" : "string",
      "enum": ["OBS", "INT", "PRD", "PRM"]
    },
    "SPW" : {
      "type" : "object",
      "description" : "Space Weather Data Record",
      "properties" : {
        "DATE" : {
                "type" : "string",
                "description" : "Date in ISO 8601 format"
              },
        "BSRN" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Bartels Solar Rotation Number"
              },
        "ND" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Day within Bartels cycle (1-27)"
              },
        "KP1" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 0000-0300 UT, multiplied by 10"
              },
        "KP2" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 0300-0600 UT, multiplied by 10"
              },
        "KP3" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 0600-0900 UT, multiplied by 10"
              },
        "KP4" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 0900-1200 UT, multiplied by 10"
              },
        "KP5" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 1200-1500 UT, multiplied by 10"
              },
        "KP6" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 1500-1800 UT, multiplied by 10"
              },
        "KP7" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 1800-2100 UT, multiplied by 10"
              },
        "KP8" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary 3-hour Range Index (Kp) for 2100-0000 UT, multiplied by 10"
              },
        "KP_SUM" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Sum of the 8 Kp indices for the day"
              },
        "AP1" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 0000-0300 UT"
              },
        "AP2" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 0300-0600 UT"
              },
        "AP3" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 0600-0900 UT"
              },
        "AP4" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 0900-1200 UT"
              },
        "AP5" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 1200-1500 UT"
              },
        "AP6" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 1500-1800 UT"
              },
        "AP7" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 1800-2100 UT"
              },
        "AP8" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Planetary Equivalent Amplitude (Ap) for 2100-0000 UT"
              },
        "AP_AVG" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "Arithmetic average of the 8 Ap indices for the day"
              },
        "CP" : {
                "type" : "number",
                "description" : "Planetary Daily Character Figure (0.0 to 2.5)"
              },
        "C9" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "C9 index (0-9)"
              },
        "ISN" : {
                "type" : "integer", "minimum" : -2147483648, "maximum" : 2147483647,
                "description" : "International Sunspot Number"
              },
        "F107_OBS" : {
                "type" : "number",
                "description" : "Observed 10.7cm Solar Radio Flux"
              },
        "F107_ADJ" : {
                "type" : "number",
                "description" : "Adjusted 10.7cm Solar Radio Flux (to 1 AU)"
              },
        "F107_DATA_TYPE" : {
                "$ref" : "#/definitions/F107DataType",
                "description" : "F10.7 Data Type"
              },
        "F107_OBS_CENTER81" : {
                "type" : "number",
                "description" : "81-day centered average of observed F10.7"
              },
        "F107_OBS_LAST81" : {
                "type" : "number",
                "description" : "81-day trailing average of observed F10.7"
              },
        "F107_ADJ_CENTER81" : {
                "type" : "number",
                "description" : "81-day centered average of adjusted F10.7"
              },
        "F107_ADJ_LAST81" : {
                "type" : "number",
                "description" : "81-day trailing average of adjusted F10.7"
              }
      },
      "additionalProperties" : false
    },
    "SPWCOLLECTION" : {
      "type" : "object",
      "properties" : {
        "RECORDS" : {
                "type" : "array", "items" : {"$ref" : "#/definitions/SPW"}
              }
      },
      "additionalProperties" : false
    }
  },
  "$ref" : "#/definitions/SPW"
}
