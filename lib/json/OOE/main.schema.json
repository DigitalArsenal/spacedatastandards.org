{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "definitions": {
    "eventCategory" : {
      "type" : "string",
      "enum": ["ANOMALY", "FAILURE", "RETIREMENT", "DEORBIT", "BREAKUP", "COLLISION", "STATUS_CHANGE", "REPOSITIONING", "UNKNOWN"]
    },
    "eventResult" : {
      "type" : "string",
      "enum": ["TOTAL_LOSS", "PARTIAL_LOSS", "DEGRADED", "RECOVERED", "NOMINAL", "PENDING"]
    },
    "OOE" : {
      "type" : "object",
      "description" : "On-Orbit Event",
      "properties" : {
        "ID" : {
                "type" : "string",
                "description" : "Unique identifier"
              },
        "SAT_NO" : {
                "type" : "integer", "minimum" : 0, "maximum" : 4294967295,
                "description" : "Satellite catalog number"
              },
        "ORIG_OBJECT_ID" : {
                "type" : "string",
                "description" : "International designator"
              },
        "DERIVED_FROM" : {
                "type" : "string",
                "description" : "Source record this was derived from"
              },
        "DECLASSIFICATION_DATE" : {
                "type" : "string",
                "description" : "Classification date (ISO 8601)"
              },
        "DECLASSIFICATION_STRING" : {
                "type" : "string",
                "description" : "Classification marking"
              },
        "EVENT_TIME" : {
                "type" : "string",
                "description" : "Event time (ISO 8601)"
              },
        "EVENT_TIME_NOTES" : {
                "type" : "string",
                "description" : "Notes on event time accuracy"
              },
        "CATEGORY" : {
                "$ref" : "#/definitions/eventCategory",
                "description" : "Event category"
              },
        "RESULT" : {
                "$ref" : "#/definitions/eventResult",
                "description" : "Event result/outcome"
              },
        "EVENT_TYPE" : {
                "type" : "string",
                "description" : "Event type detail"
              },
        "OPERATOR_ORG_ID" : {
                "type" : "string",
                "description" : "Operator organization identifier"
              },
        "OWNER_ORG_ID" : {
                "type" : "string",
                "description" : "Owner organization identifier"
              },
        "LESSEE_ORG_ID" : {
                "type" : "string",
                "description" : "Lessee organization identifier"
              },
        "OPERATED_ON_BEHALF_OF_ORG_ID" : {
                "type" : "string",
                "description" : "Operated on behalf of organization"
              },
        "GEO_POSITION" : {
                "type" : "number",
                "description" : "GEO longitude at event time (degrees east)"
              },
        "PLANE_SLOT" : {
                "type" : "string",
                "description" : "Orbital plane slot"
              },
        "PLANE_NUMBER" : {
                "type" : "string",
                "description" : "Orbital plane number"
              },
        "POSITION_STATUS" : {
                "type" : "string",
                "description" : "Position status at event time"
              },
        "UNTIL_TIME" : {
                "type" : "string",
                "description" : "Time until expected recovery (ISO 8601)"
              },
        "OFFICIAL_LOSS_DATE" : {
                "type" : "string",
                "description" : "Official loss date (ISO 8601)"
              },
        "NET_AMOUNT" : {
                "type" : "number",
                "description" : "Financial loss amount (USD)"
              },
        "UNDERLYING_CAUSE" : {
                "type" : "string",
                "description" : "Root cause description"
              },
        "CAPABILITY_LOSS" : {
                "type" : "number",
                "description" : "Capability loss fraction (0-1)"
              },
        "CAPACITY_LOSS" : {
                "type" : "number",
                "description" : "Capacity loss fraction (0-1)"
              },
        "INSURANCE_LOSS" : {
                "type" : "number",
                "description" : "Insurance loss amount (USD)"
              },
        "THIRD_PARTY_INSURANCE_LOSS" : {
                "type" : "number",
                "description" : "Third-party insurance loss (USD)"
              },
        "INJURED" : {
                "type" : "integer", "minimum" : 0, "maximum" : 65535,
                "description" : "Number of personnel injured"
              },
        "KILLED" : {
                "type" : "integer", "minimum" : 0, "maximum" : 65535,
                "description" : "Number of fatalities"
              },
        "AGE_AT_EVENT" : {
                "type" : "number",
                "description" : "Spacecraft age at event (years)"
              },
        "LIFE_LOST" : {
                "type" : "number",
                "description" : "Design life remaining at event (years)"
              },
        "ACHIEVED_FLIGHT_PHASE" : {
                "type" : "string",
                "description" : "Flight phase achieved"
              },
        "OCCURRENCE_FLIGHT_PHASE" : {
                "type" : "string",
                "description" : "Flight phase at occurrence"
              },
        "STAGE_AT_FAULT" : {
                "type" : "string",
                "description" : "Stage at fault"
              },
        "EQUIPMENT_AT_FAULT" : {
                "type" : "string",
                "description" : "Equipment at fault"
              },
        "EQUIPMENT_TYPE_AT_FAULT" : {
                "type" : "string",
                "description" : "Equipment type at fault"
              },
        "EQUIPMENT_PART_AT_FAULT" : {
                "type" : "string",
                "description" : "Equipment part at fault"
              },
        "CONSEQUENTIAL_EQUIPMENT_FAILURE" : {
                "type" : "string",
                "description" : "Consequential equipment failure"
              },
        "INCLINED" : {
                "type" : "boolean",
                "description" : "True if orbit is inclined"
              },
        "DESCRIPTION" : {
                "type" : "string",
                "description" : "Event description"
              },
        "REMARKS" : {
                "type" : "string",
                "description" : "Additional remarks"
              },
        "OBJECT_STATUS" : {
                "type" : "string",
                "description" : "Object status after event"
              },
        "SATELLITE_POSITION" : {
                "type" : "string",
                "description" : "Satellite position after event"
              },
        "ON_ORBIT" : {
                "type" : "string",
                "description" : "On-orbit reference"
              }
      },
      "additionalProperties" : false
    }
  },
  "$ref" : "#/definitions/OOE"
}
