    "TIM" : {
      "type" : "object",
      "description" : "Time System",
      "properties" : {
        "TIME_SYSTEM" : {
                "$ref" : "#/definitions/timeSystem"
              }
      },
      "additionalProperties" : false
    },
    "MET" : {
      "type" : "object",
      "description" : "Mean Element Theory",
      "properties" : {
        "MEAN_ELEMENT_THEORY" : {
                "$ref" : "#/definitions/meanElementTheory"
              }
      },
      "additionalProperties" : false
