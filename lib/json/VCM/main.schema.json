    "lunarPerturbationModel" : {
      "type" : "string",
      "enum": ["NONE", "DE430", "DE431", "LP150Q"]
    },
    "solarPerturbationModel" : {
      "type" : "string",
      "enum": ["NONE", "DE430", "DE431"]
    },
    "resonanceModel" : {
      "type" : "string",
      "enum": ["NONE", "HIGH_ALTITUDE_RESONANCE", "LOW_ALTITUDE_RESONANCE", "LUNAR_RESONANCE", "SOLAR_RESONANCE"]
    },
    "perturbationStatus" : {
      "type" : "string",
      "enum": ["OFF", "ON"]
