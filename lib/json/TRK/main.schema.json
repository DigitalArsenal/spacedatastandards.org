                "type" : "integer", "minimum" : 0, "maximum" : 65535,
                "description" : "Mode 1 code"
              },
        "M1V" : {
                "type" : "integer", "minimum" : 0, "maximum" :255,
                "description" : "Mode 1 validity"
              },
        "M2" : {
                "type" : "integer", "minimum" : 0, "maximum" : 65535,
                "description" : "Mode 2 code"
              },
        "M2V" : {
                "type" : "integer", "minimum" : 0, "maximum" :255,
                "description" : "Mode 2 validity"
              },
        "M3A" : {
                "type" : "integer", "minimum" : 0, "maximum" : 65535,
                "description" : "Mode 3A code"
