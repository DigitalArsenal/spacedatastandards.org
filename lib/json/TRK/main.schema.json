              },
        "OBJ_ID" : {
                "type" : "string",
                "description" : "Object identifier"
              },
        "OBJ_TYPE" : {
                "type" : "string",
                "description" : "Object type classification"
              },
        "OBJ_SPEC" : {
                "type" : "string",
                "description" : "Object specific type"
              },
        "OBJ_PLAT" : {
                "type" : "string",
