{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "definitions": {
    "TrkTrackStatus" : {
      "type" : "string",
      "enum": ["ACTIVE", "DROPPED", "TENTATIVE", "CONFIRMED", "COASTED", "DEAD"]
    },
    "trackEnvironment" : {
      "type" : "string",
      "enum": ["SPACE", "AIR", "SURFACE", "SUBSURFACE", "LAND", "UNKNOWN"]
    },
    "TRK" : {
      "type" : "object",
      "description" : "Track",
      "properties" : {
        "ID" : {
                "type" : "string",
                "description" : "Unique identifier"
              },
        "CNTCT" : {
                "type" : "string",
                "description" : "Contact reference"
              },
        "MSG_TS" : {
                "type" : "string",
                "description" : "Message timestamp (ISO 8601)"
              },
        "MSN_ID" : {
                "type" : "string",
                "description" : "Mission identifier"
              },
        "ASSET_NAT" : {
                "type" : "string",
                "description" : "Asset nationality"
              },
        "ASSET" : {
                "type" : "string",
