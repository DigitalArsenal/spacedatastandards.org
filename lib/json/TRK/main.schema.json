              },
        "LC_VEL" : {
                "type" : "array", "items" : {"type" : "number"},
                "description" : "Local coordinate velocity as flat array"
              },
        "LC_ACC" : {
                "type" : "array", "items" : {"type" : "number"},
                "description" : "Local coordinate acceleration as flat array"
              },
        "COV" : {
                "type" : "array", "items" : {"type" : "number"},
                "description" : "Covariance data (21 elements per point for 6x6 lower triangular)"
              },
        "ERR_ELLP" : {
                "type" : "array", "items" : {"type" : "number"},
                "description" : "Error ellipse data (6 elements per point)"
              }
