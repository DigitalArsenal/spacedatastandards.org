              },
        "SENSOR_ID" : {
                "type" : "string",
                "description" : "Sensor identifier"
              },
        "SEN_QUAL" : {
                "type" : "string",
                "description" : "Sensor quality assessment"
              },
        "TRK_ID" : {
                "type" : "string",
